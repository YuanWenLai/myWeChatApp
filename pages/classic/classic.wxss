.container{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.nav{
  width: 100%;
  display: flex;
  flex-direction: row;
  height: 52rpx;
  align-items: center;
  justify-content: space-between;
  border-top: 1px solid #f5f5f5;
  border-bottom: 1px solid #f5f5f5;
}

.navi{
  position: absolute;
  bottom: 100rpx;
}

.nav-right{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.share-img image{
  height: 42rpx;
  width: 42rpx;
  padding: 0 10rpx;
  position: relative;
  top:8rpx;
}