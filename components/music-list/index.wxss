.music-list{
  width: 100%;
  height: 100%;
  display: flex;
  position: relative;
  left: 30rpx;
  flex-direction: column;
  justify-content: center;
  margin-bottom: 36rpx;
}

.music-title{
  font-size: 34rpx;
  font-weight: 550;
  color: rgba(17, 17, 17, 0.79);
}

.music-album{
  width: 100%;
  height: 30rpx;
  display: flex;
  flex-direction: row;
  margin-top: 16rpx;
  font-size: 26rpx;
  color: #999999;
  align-items: center;
}

.music-album image{
  width: 60rpx;
  height: 30rpx;
  margin-right: 10rpx;
  border-radius: 6rpx;
}